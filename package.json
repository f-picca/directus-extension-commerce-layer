{
	"name": "directus-extension-commerce-layer",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "endpoint",
				"name": "commerce-layer-endpoint",
				"source": "src/commerce-layer-endpoint/index.ts"
			},
			{
				"type": "interface",
				"name": "commerce-layer-commerce-data-interface",
				"source": "src/commerce-layer-commerce-data-interface/index.ts"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.1.1",
		"@types/node": "^22.15.27",
		"typescript": "^5.8.3",
		"vue": "^3.5.16"
	}
}
